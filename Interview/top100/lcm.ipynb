     ]
    }
   ],
   "source": [
    "n1,n2=12,78\n",
    "maxnum=max(n1,n2)\n",
    "while True:\n",
    "    if maxnum%n1==0 and maxnum%n2==0:\n",
    "        break\n",
    "    maxnum+=1\n",
    "print(maxnum)"
   ]
